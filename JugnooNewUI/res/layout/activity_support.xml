<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/relative"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical" >

        <RelativeLayout
            android:id="@+id/topBar"
            android:layout_width="720px"
            android:layout_height="96px"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:background="@color/yellow" >

            <TextView
                android:id="@+id/textViewTitle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
                android:text="SUPPORT"
                android:textColor="@color/white"
                android:textSize="30.6px" />

            <ImageView
                android:id="@+id/imageViewBack"
                android:layout_width="60.7px"
                android:layout_height="76.9px"
                android:layout_centerVertical="true"
                android:layout_marginLeft="10px"
                android:padding="20px"
                android:src="@drawable/back_white_selector" />
        </RelativeLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:splitMotionEvents="false" 
            android:background="@color/white_light_grey"
            
            >

            <RelativeLayout
                android:id="@+id/relativeLayoutSendUsEmail"
                android:layout_width="match_parent"
                android:layout_height="120px"
                android:background="@drawable/background_white_yellow_selector" >

                <TextView
                    android:id="@+id/textViewSendUsEmail"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="30px"
                    android:text="Send us Email"
                    android:textColor="@drawable/text_color_grey_dark_white_selector"
                    android:textSize="34px" />

                <ImageView
                    android:layout_width="13px"
                    android:layout_height="26px"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="40px"
                    android:src="@drawable/arrow_icon_selector"
                    android:tag="mwar" />

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="2px"
                    android:layout_alignParentBottom="true"
                    android:src="@color/yellow" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/relativeLayoutCallUs"
                android:layout_width="match_parent"
                android:layout_height="120px"
                android:background="@drawable/background_white_yellow_selector" >

                <TextView
                    android:id="@+id/textViewCallUs"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="30px"
                    android:text="Call Us"
                    android:textColor="@drawable/text_color_grey_dark_white_selector"
                    android:textSize="34px" />

                <ImageView
                    android:layout_width="13px"
                    android:layout_height="26px"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="40px"
                    android:src="@drawable/arrow_icon_selector"
                    android:tag="mwar" />

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="2px"
                    android:layout_alignParentBottom="true"
                    android:src="@color/yellow" />
            </RelativeLayout>
            
            <RelativeLayout
                android:id="@+id/relativeLayoutFAQ"
                android:layout_width="match_parent"
                android:layout_height="120px"
                android:background="@drawable/background_white_yellow_selector" >

                <TextView
                    android:id="@+id/textViewFAQ"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="30px"
                    android:text="FAQs"
                    android:textColor="@drawable/text_color_grey_dark_white_selector"
                    android:textSize="34px" />

                <ImageView
                    android:layout_width="13px"
                    android:layout_height="26px"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="40px"
                    android:src="@drawable/arrow_icon_selector"
                    android:tag="mwar" />

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="2px"
                    android:layout_alignParentBottom="true"
                    android:src="@color/yellow" />
            </RelativeLayout>
            
            <RelativeLayout
                android:id="@+id/relativeLayoutFeedback"
                android:layout_width="match_parent"
                android:layout_height="120px"
                android:background="@drawable/background_white_yellow_selector" >

                <TextView
                    android:id="@+id/textViewFeedback"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="30px"
                    android:text="Feedback"
                    android:textColor="@drawable/text_color_grey_dark_white_selector"
                    android:textSize="34px" />

                <ImageView
                    android:layout_width="13px"
                    android:layout_height="26px"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="40px"
                    android:src="@drawable/arrow_icon_selector"
                    android:tag="mwar" />

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="2px"
                    android:layout_alignParentBottom="true"
                    android:src="@color/yellow" />
            </RelativeLayout>
            
        </LinearLayout>
    </LinearLayout>

</RelativeLayout>