        <ImageView
            android:id="@+id/imageViewAutos2"
            android:layout_width="112px"
            android:layout_height="112px"
            android:src="@drawable/jugnoo_icon"
            android:tag="mwar"
            android:visibility="gone" />
    </RelativeLayout>

</RelativeLayout>