        android:layout_width="112px"
        android:layout_height="550px">

        <ImageView
            android:id="@+id/imageViewMeals1"
            android:layout_width="112px"
            android:layout_height="112px"
            android:layout_marginTop="400px"
            android:src="@drawable/jugnoo_icon"
            android:tag="mwar"
            android:visibility="gone" />

        <ImageView
            android:id="@+id/imageViewFatafat1"
            android:layout_width="112px"
            android:layout_height="112px"
            android:layout_marginTop="400px"
            android:src="@drawable/jugnoo_icon"
            android:tag="mwar"
            android:visibility="gone" />

        <ImageView
            android:id="@+id/imageViewAutos1"
