            android:src="@drawable/jugnoo_icon"
            android:tag="mwar"
            android:visibility="gone" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/relativeDownward"
        android:layout_width="112px"
        android:layout_height="550px">

        <ImageView
