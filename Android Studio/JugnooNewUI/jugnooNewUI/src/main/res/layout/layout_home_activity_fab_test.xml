<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:fab="http://schemas.android.com/apk/res-auto"
    android:id="@+id/relativeLayoutFABTest"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/transparent"
    android:splitMotionEvents="false"
    android:visibility="visible">

    <product.clicklabs.jugnoo.widgets.FAB.FloatingActionMenu
        android:id="@+id/menu_labels_right_Test"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentBottom="true"
        android:paddingBottom="23dp"
        android:contentDescription="@string/jugnoo_offerings"
        android:paddingLeft="20dp"
        android:tag="DontTouchMe"
        fab:menu_icon="@drawable/ic_fab_jeanie"
        fab:menu_labels_colorNormal="@color/white"
        fab:fab_colorRipple="@color/grey_light_alpha"
        fab:menu_labels_colorPressed="@color/grey_light"
        fab:menu_shadowColor="@color/black_50"
        fab:menu_animationDelayPerItem="25"
        fab:menu_labels_ellipsize="end"
        fab:menu_labels_singleLine="true"
        fab:menu_backgroundColor="@color/black_70"
        fab:menu_labels_position="right">

        <product.clicklabs.jugnoo.widgets.FAB.FloatingActionButton
            android:id="@+id/fabPayTest"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/ic_fab_pay"
            fab:fab_size="mini"
            android:tag="DontTouchMe"
            fab:fab_shadowXOffset="1dp"
            fab:fab_shadowYOffset="2dp"
            fab:fab_shadowColor="@color/black_50"
            fab:fab_colorNormal="@color/yellow_pay_fab"
            fab:fab_colorPressed="@color/yellow_pay_fab_pressed"
            fab:fab_colorRipple="@color/yellow_pay_fab_pressed"
            fab:fab_label="@string/pay" />

        <product.clicklabs.jugnoo.widgets.FAB.FloatingActionButton
            android:id="@+id/fabMenusTest"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/ic_fab_menus"
            fab:fab_size="mini"
            android:tag="DontTouchMe"
            fab:fab_shadowXOffset="1dp"
            fab:fab_shadowYOffset="2dp"
            fab:fab_shadowColor="@color/black_50"
            fab:fab_colorNormal="@color/purple_menus_fab"
            fab:fab_colorPressed="@color/purple_menus_fab_pressed"
            fab:fab_colorRipple="@color/purple_menus_fab_pressed"
            fab:fab_label="@string/menus" />

        <product.clicklabs.jugnoo.widgets.FAB.FloatingActionButton
            android:id="@+id/fabGroceryTest"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/ic_fab_grocery"
            fab:fab_size="mini"
            android:tag="DontTouchMe"
            fab:fab_shadowXOffset="1dp"
            fab:fab_shadowYOffset="2dp"
            fab:fab_shadowColor="@color/black_50"
            fab:fab_colorNormal="@color/white"
            fab:fab_colorPressed="@color/grey_light"
            fab:fab_colorRipple="@color/grey_light_alpha"
            fab:fab_label="Grocery" />

        <product.clicklabs.jugnoo.widgets.FAB.FloatingActionButton
            android:id="@+id/fabFreshTest"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/ic_fab_fresh"
            fab:fab_size="mini"
            android:tag="DontTouchMe"
            fab:fab_shadowXOffset="1dp"
            fab:fab_shadowYOffset="2dp"
            fab:fab_shadowColor="@color/black_50"
            fab:fab_colorNormal="@color/green_fresh_fab"
            fab:fab_colorPressed="@color/green_fresh_fab_pressed"
            fab:fab_colorRipple="@color/green_fresh_fab_pressed"
            fab:fab_label="Fresh" />

        <product.clicklabs.jugnoo.widgets.FAB.FloatingActionButton
            android:id="@+id/fabMealsTest"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/ic_fab_meals"
            fab:fab_size="mini"
            fab:fab_shadowXOffset="1dp"
            fab:fab_shadowYOffset="2dp"
            fab:fab_shadowColor="@color/black_50"
            fab:fab_colorNormal="@color/pink_meals_fab"
            fab:fab_colorPressed="@color/pink_meals_fab_pressed"
            fab:fab_colorRipple="@color/pink_meals_fab_pressed"
            android:tag="DontTouchMe"
            fab:fab_label="Meals"/>


        <product.clicklabs.jugnoo.widgets.FAB.FloatingActionButton
            android:id="@+id/fabAutosTest"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/ic_fab_autos"
            fab:fab_size="mini"
            fab:fab_shadowXOffset="1dp"
            fab:fab_shadowYOffset="2dp"
            fab:fab_shadowColor="@color/black_50"
            android:tag="DontTouchMe"
            fab:fab_colorNormal="@color/theme_color"
            fab:fab_colorPressed="@color/orange_rides_fab_pressed"
            fab:fab_colorRipple="@color/orange_rides_fab_pressed"
            fab:fab_label="Rides" />


    </product.clicklabs.jugnoo.widgets.FAB.FloatingActionMenu>

    <!--android:layout_marginBottom="@dimen/px_170"-->
    <RelativeLayout
        android:id="@+id/rlGenieHelp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/px_20"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="150px"
        android:visibility="gone">

        <ImageView
            android:layout_width="40px"
            android:layout_height="40px"
            android:layout_marginLeft="60px"
            android:layout_below="@+id/tvGenieHelp"
            android:background="@drawable/triangle_help_text"
            android:rotation="180"
            android:tag="mwar" />

        <TextView
            android:id="@+id/tvGenieHelp"
            android:layout_width="@dimen/px_400"
            android:layout_height="wrap_content"
            android:background="@drawable/capsule_black_70"
            android:padding="15px"
            android:text="Explore more from Jugnoo"
            android:textColor="@color/white"
            android:textSize="@dimen/text_size_30" />
    </RelativeLayout>

</RelativeLayout>