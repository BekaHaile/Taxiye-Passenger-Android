<LinearLayout
    android:id="@+id/relative"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/white">

    <RelativeLayout
        android:id="@+id/topBar"
        android:layout_width="720px"
        android:layout_height="96px"
        android:background="@color/yellow">

        <TextView
            android:id="@+id/textViewTitle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:text="EMERGENCY CONTACT"
            android:textColor="@color/white"
            android:textSize="30.6px"/>

        <ImageView
            android:id="@+id/imageViewBack"
            android:layout_width="60.7px"
            android:layout_height="76.9px"
            android:layout_centerVertical="true"
            android:layout_marginLeft="10px"
            android:padding="20px"
            android:src="@drawable/back_white_selector"/>
    </RelativeLayout>

    <ScrollView
        android:id="@+id/scrollView"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/white_light_grey"
            android:orientation="vertical"
            android:splitMotionEvents="false"

            >

            <RelativeLayout
                android:id="@+id/relativeLayoutEmergencyContact1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="51px"
                >
                <RelativeLayout
                    android:id="@+id/relativeLayoutEmergencyContact1Inner"
                    android:layout_width="match_parent"
                    android:layout_height="80px"
                    android:background="@color/white">


                </RelativeLayout>


                <TextView
                    android:id="@+id/textViewRateUs"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="30px"
                    android:text="Rate us on Play Store"
                    android:textColor="@drawable/text_color_grey_dark_white_selector"
                    android:textSize="34px"/>

                <ImageView
                    android:layout_width="13px"
                    android:layout_height="26px"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="40px"
                    android:src="@drawable/arrow_icon_selector"
                    android:tag="mwar"/>

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="2px"
                    android:layout_alignParentBottom="true"
                    android:src="@color/yellow"/>
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/relativeLayoutLikeUs"
                android:layout_width="match_parent"
                android:layout_height="120px"
                android:background="@drawable/background_white_yellow_selector">

                <TextView
                    android:id="@+id/textViewLikeUs"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="30px"
                    android:text="Like us on Facebook"
                    android:textColor="@drawable/text_color_grey_dark_white_selector"
                    android:textSize="34px"/>

                <ImageView
                    android:layout_width="13px"
                    android:layout_height="26px"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="40px"
                    android:src="@drawable/arrow_icon_selector"
                    android:tag="mwar"/>

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="2px"
                    android:layout_alignParentBottom="true"
                    android:src="@color/yellow"/>
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/relativeLayoutTNC"
                android:layout_width="match_parent"
                android:layout_height="120px"
                android:background="@drawable/background_white_yellow_selector">

                <TextView
                    android:id="@+id/textViewTNC"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="30px"
                    android:text="Terms and Conditions"
                    android:textColor="@drawable/text_color_grey_dark_white_selector"
                    android:textSize="34px"/>

                <ImageView
                    android:layout_width="13px"
                    android:layout_height="26px"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="40px"
                    android:src="@drawable/arrow_icon_selector"
                    android:tag="mwar"/>

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="2px"
                    android:layout_alignParentBottom="true"
                    android:src="@color/yellow"/>
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/relativeLayoutPrivacy"
                android:layout_width="match_parent"
                android:layout_height="120px"
                android:background="@drawable/background_white_yellow_selector">

                <TextView
                    android:id="@+id/textViewPrivacy"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="30px"
                    android:text="Privacy Policy"
                    android:textColor="@drawable/text_color_grey_dark_white_selector"
                    android:textSize="34px"/>

                <ImageView
                    android:layout_width="13px"
                    android:layout_height="26px"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="40px"
                    android:src="@drawable/arrow_icon_selector"
                    android:tag="mwar"/>

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="2px"
                    android:layout_alignParentBottom="true"
                    android:src="@color/yellow"/>
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/relativeLayoutAbout"
                android:layout_width="match_parent"
                android:layout_height="120px"
                android:background="@drawable/background_white_yellow_selector">

                <TextView
                    android:id="@+id/textViewAbout"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="30px"
                    android:text="About"
                    android:textColor="@drawable/text_color_grey_dark_white_selector"
                    android:textSize="34px"/>

                <ImageView
                    android:layout_width="13px"
                    android:layout_height="26px"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="40px"
                    android:src="@drawable/arrow_icon_selector"
                    android:tag="mwar"/>

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="2px"
                    android:layout_alignParentBottom="true"
                    android:src="@color/yellow"/>
            </RelativeLayout>

        </LinearLayout>

    </ScrollView>

</LinearLayout>
