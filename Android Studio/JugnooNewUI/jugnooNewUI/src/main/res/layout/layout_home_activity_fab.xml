<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:fab="http://schemas.android.com/apk/res-auto"
    android:id="@+id/relativeLayoutFAB"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:tag="DontTouchMe"
    android:background="@color/transparent"
    android:animateLayoutChanges="true"
    android:splitMotionEvents="false"
    android:visibility="visible">

    <View
        android:id="@+id/fabExtra"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="gone"
        android:background="@color/transparent"/>

    <product.clicklabs.jugnoo.widgets.FAB.FloatingActionMenu
        android:id="@+id/menu_labels_right"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_above="@+id/linearLayoutCheckoutContainer"
        android:paddingBottom="80dp"
        android:paddingLeft="15dp"
        android:tag="DontTouchMe"
        fab:menu_buttonSpacing="0px"
        fab:menu_icon="@drawable/ic_fab_menu"
        fab:menu_animationDelayPerItem="50"
        fab:menu_labels_ellipsize="end"
        fab:menu_labels_singleLine="true"
        fab:menu_backgroundColor="#ccffffff"
        fab:menu_labels_position="right">

        <product.clicklabs.jugnoo.widgets.FAB.FloatingActionButton
            android:id="@+id/fabDelivery"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/ic_fab_delivery_selector"
            android:layout_marginBottom="0px"
            fab:fab_colorNormal="#0000"
            fab:fab_colorPressed="#0000"
            fab:fab_colorRipple="#0000"
            android:tag="DontTouchMe"
            fab:fab_size="mini"
            fab:fab_label="Delivery" />

        <product.clicklabs.jugnoo.widgets.FAB.FloatingActionButton
            android:id="@+id/fabMeals"
            android:layout_width="45dp"
            android:layout_height="45dp"
            android:src="@drawable/ic_fab_meals_selector"
            android:layout_marginBottom="0px"
            android:tag="DontTouchMe"
            fab:fab_colorNormal="#0000"
            fab:fab_colorPressed="#0000"
            fab:fab_colorRipple="#0000"
            fab:fab_size="mini"
            fab:fab_label="Meals" />

        <product.clicklabs.jugnoo.widgets.FAB.FloatingActionButton
            android:id="@+id/fabFresh"
            android:layout_width="45dp"
            android:layout_height="45dp"
            android:src="@drawable/ic_fab_fresh_selector"
            android:layout_marginBottom="0px"
            fab:fab_colorNormal="#0000"
            fab:fab_colorPressed="#0000"
            fab:fab_colorRipple="#0000"
            android:tag="DontTouchMe"
            fab:fab_size="mini"
            fab:fab_label="Fresh" />

        <product.clicklabs.jugnoo.widgets.FAB.FloatingActionButton
            android:id="@+id/fabAutos"
            android:layout_width="45dp"
            android:layout_height="45dp"
            android:src="@drawable/ic_fab_auto_selector"
            android:tag="DontTouchMe"
            fab:fab_colorNormal="#0000"
            fab:fab_colorPressed="#0000"
            fab:fab_colorRipple="#0000"
            fab:fab_size="mini"
            fab:fab_label="Autos" />

    </product.clicklabs.jugnoo.widgets.FAB.FloatingActionMenu>

</RelativeLayout>