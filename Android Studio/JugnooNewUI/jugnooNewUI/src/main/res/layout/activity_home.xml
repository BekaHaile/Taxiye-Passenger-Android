                        android:layout_height="45px"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="20px"
                        android:layout_toLeftOf="@+id/imageViewWalletArrow"
                        android:visibility="gone"
                        wheel:matProg_barColor="@color/grey_dark_more"
                        wheel:matProg_barWidth="2dp"
                        wheel:matProg_fillRadius="true"
                        wheel:matProg_progressIndeterminate="true"
                        />

                    <TextView
                        android:id="@+id/textViewWalletValue"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="20px"
                        android:layout_toLeftOf="@+id/imageViewWalletArrow"
                        android:background="@drawable/background_yellow_rounded"
                        android:gravity="center_vertical|right"
                        android:padding="10px"
                        android:text="@string/rupee_0"
                        android:textColor="@color/black"
                        android:textSize="30px"
                        android:visibility="visible"
                        />

                    <ImageView
                        android:id="@+id/imageViewWalletArrow"
                        android:layout_width="13px"
                        android:layout_height="26px"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="40px"
                        android:src="@drawable/arrow_icon_selector"
                        android:tag="mwar"/>

                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="2px"
                        android:layout_alignParentBottom="true"
                        android:layout_marginLeft="60px"
                        android:background="@color/grey_light"/>
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/relativeLayoutPromotions"
                    android:layout_width="match_parent"
                    android:layout_height="100px"
                    android:background="@drawable/background_white_yellow_selector"
                    android:splitMotionEvents="false">

                    <ImageView
                        android:id="@+id/imageViewPromotions"
                        android:layout_width="40px"
                        android:layout_height="40px"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="50px"
                        android:src="@drawable/promotion_icon_selector"
                        android:tag="mwar"/>

                    <TextView
                        android:id="@+id/textViewPromotions"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="35px"
