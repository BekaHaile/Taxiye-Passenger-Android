                    <ImageView
                        android:id="@+id/imageViewSupportArrow"
                        android:layout_width="13px"
                        android:layout_height="26px"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="40px"
                        android:src="@drawable/arrow_icon_selector"
                        android:tag="mwar"/>

                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="2px"
                        android:layout_alignParentBottom="true"
                        android:layout_marginLeft="60px"
                        android:background="@color/grey_light"/>
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/relativeLayoutAbout"
                    android:layout_width="match_parent"
                    android:layout_height="100px"
                    android:background="@drawable/background_white_yellow_selector"
                    android:splitMotionEvents="false">

                    <ImageView
                        android:id="@+id/imageViewAbout"
                        android:layout_width="38px"
                        android:layout_height="38px"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="50px"
                        android:src="@drawable/about_icon_selector"
                        android:tag="mwar"/>

                    <TextView
                        android:id="@+id/textViewAbout"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="35px"
                        android:layout_toRightOf="@+id/imageViewAbout"
                        android:text="About"
                        android:textColor="@drawable/text_color_grey_dark_white_selector"
                        android:textSize="30px"/>

                    <ImageView
                        android:id="@+id/imageViewAboutArrow"
                        android:layout_width="13px"
                        android:layout_height="26px"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="40px"
                        android:src="@drawable/arrow_icon_selector"
                        android:tag="mwar"/>

                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="2px"
                        android:layout_alignParentBottom="true"
                        android:layout_marginLeft="60px"
                        android:background="@color/grey_light"
                        android:visibility="gone"/>
                </RelativeLayout>
            </LinearLayout>
        </ScrollView>
    </LinearLayout>


</android.support.v4.widget.DrawerLayout>