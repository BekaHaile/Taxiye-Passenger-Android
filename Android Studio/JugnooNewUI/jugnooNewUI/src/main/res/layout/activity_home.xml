                        android:textSize="30px"/>

                    <ImageView
                        android:id="@+id/imageViewPromotionsArrow"
                        android:layout_width="13px"
                        android:layout_height="26px"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="40px"
                        android:src="@drawable/arrow_icon_selector"
                        android:tag="mwar"/>

                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="2px"
                        android:layout_alignParentBottom="true"
                        android:layout_marginLeft="60px"
                        android:background="@color/grey_light"/>
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/relativeLayoutNotificationMenu"
                    android:layout_width="match_parent"
                    android:layout_height="100px"
                    android:background="@drawable/background_white_yellow_selector"
                    android:splitMotionEvents="false"
                    android:visibility="visible"
                    >

                    <ImageView
                        android:id="@+id/imageViewNotificationMenu"
                        android:layout_width="40px"
                        android:layout_height="45px"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="50px"
                        android:src="@drawable/ic_notification_center_thin_selector"
                        android:tag="mwar"/>

                    <TextView
                        android:id="@+id/textViewNotificationMenu"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="35px"
                        android:layout_toRightOf="@+id/imageViewNotificationMenu"
                        android:text="Notification Center"
                        android:textColor="@drawable/text_color_grey_dark_white_selector"
                        android:textSize="30px"/>

                    <TextView
                        android:id="@+id/textViewNotificationValueMenu"
                        android:layout_width="55px"
                        android:layout_height="55px"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="20px"
                        android:layout_toLeftOf="@+id/imageViewNotificationArrowMenu"
                        android:background="@drawable/circle_yellow"
                        android:gravity="center"
                        android:padding="10px"
                        android:tag="mwar"
                        android:text="0"
                        android:textColor="@color/black"
                        android:textSize="30px"/>

                    <ImageView
                        android:id="@+id/imageViewNotificationArrowMenu"
                        android:layout_width="13px"
                        android:layout_height="26px"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="40px"
                        android:src="@drawable/arrow_icon_selector"
                        android:tag="mwar"/>

                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="2px"
                        android:layout_alignParentBottom="true"
                        android:layout_marginLeft="60px"
                        android:background="@color/grey_light"/>
                </RelativeLayout>


                <RelativeLayout
                    android:id="@+id/relativeLayoutTransactions"
                    android:layout_width="match_parent"
                    android:layout_height="100px"
                    android:background="@drawable/background_white_yellow_selector"
                    android:splitMotionEvents="false">

                    <ImageView
                        android:id="@+id/imageViewTransactions"
                        android:layout_width="38px"
                        android:layout_height="47px"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="50px"
                        android:src="@drawable/transactions_icon_selector"
                        android:tag="mwar"/>

                    <TextView
                        android:id="@+id/textViewTransactions"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="35px"
                        android:layout_toRightOf="@+id/imageViewTransactions"
                        android:text="Ride History"
                        android:textColor="@drawable/text_color_grey_dark_white_selector"
                        android:textSize="30px"/>

                    <ImageView
                        android:layout_width="13px"
                        android:layout_height="26px"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="40px"
                        android:src="@drawable/arrow_icon_selector"
                        android:tag="mwar"/>

                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="2px"
                        android:layout_alignParentBottom="true"
                        android:layout_marginLeft="60px"
                        android:background="@color/grey_light"/>
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/relativeLayoutFareEstimate"
