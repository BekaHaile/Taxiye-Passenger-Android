<android.support.v4.widget.DrawerLayout
    android:id="@+id/drawerLayout"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    >

    <FrameLayout
        android:id="@+id/container"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:splitMotionEvents="false">

        <com.sothree.slidinguppanel.SlidingUpPanelLayout
            xmlns:sothree="http://schemas.android.com/apk/res-auto"
            android:id="@+id/slidingLayout"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="bottom"
            sothree:umanoFadeColor="@color/transparent"
            sothree:umanoPanelHeight="110px"
            sothree:umanoShadowHeight="10px">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:splitMotionEvents="false"
            >

            <!-- map layout -->
            <RelativeLayout
                android:id="@+id/mapLayout"
                android:layout_width="match_parent"
                android:layout_height="@dimen/screen_height"
                android:splitMotionEvents="false"
                android:layout_marginTop="@dimen/top_bar_height"
                android:visibility="visible">

                <fragment
                    android:id="@+id/map"
                    class="product.clicklabs.jugnoo.utils.TouchableMapFragment"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"/>
            </RelativeLayout>

            <!-- passenger main layout -->
            <RelativeLayout
                android:id="@+id/passengerMainLayout"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:splitMotionEvents="false"
                android:visibility="visible"
                android:layout_marginTop="@dimen/top_bar_height"
                >

                <!-- centre pin layout -->
                <RelativeLayout
                    android:id="@+id/centreLocationRl"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:splitMotionEvents="false"
                    android:visibility="gone">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:splitMotionEvents="false"
                        android:layout_above="@+id/imageViewCenterPinMargin"
                        >
                        <RelativeLayout
                            android:id="@+id/relativeLayoutCentrePin"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_above="@+id/centreLocationPivot"
                            android:layout_centerHorizontal="true">

                            <ImageView
                                android:id="@+id/centreLocationPin"
                                android:layout_width="77px"
                                android:layout_height="130px"
                                android:background="@drawable/ic_centre_pin_big"
                                android:tag="mwar"/>

                            <TextView
                                android:id="@+id/textViewCentrePinETA"
                                android:layout_width="77px"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="8px"
                                android:gravity="center"
                                android:text="-"
                                android:textColor="@color/white"
                                android:textSize="28px"
                                />

                            <TextView
                                android:id="@+id/textViewCentrePinETAMin"
                                android:layout_width="77px"
                                android:layout_height="wrap_content"
                                android:layout_below="@+id/textViewCentrePinETA"
                                android:layout_marginTop="-6px"
                                android:gravity="center"
                                android:text="MIN"
                                android:textColor="@color/white"
                                android:textSize="22px"
                                />

                        </RelativeLayout>

                        <ImageView
                            android:id="@+id/centreLocationPivot"
                            android:layout_width="1px"
                            android:layout_height="1px"
                            android:layout_centerInParent="true"/>
                    </RelativeLayout>
                    <ImageView
                        android:id="@+id/imageViewCenterPinMargin"
                        android:layout_width="match_parent"
                        android:layout_height="0px"
                        android:layout_alignParentBottom="true"/>

                </RelativeLayout>

                <!-- initial layout -->
                <include layout="@layout/layout_home_activity_initial"/>

                <!-- assigning layout -->
                <include layout="@layout/layout_home_activity_assigning"/>

                <!-- request final layout -->
                <include layout="@layout/layout_home_activity_request_final"/>

            </RelativeLayout>

            <!-- search layout -->
            <RelativeLayout
                android:id="@+id/relativeLayoutSearch"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="@dimen/top_bar_height"
                android:background="@color/white_translucent"
                android:splitMotionEvents="false"
                android:visibility="gone">

            </RelativeLayout>

            <!-- end ride layout -->
            <RelativeLayout
                android:id="@+id/endRideReviewRl"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="@dimen/top_bar_height"
                android:splitMotionEvents="false"
                android:visibility="gone">

                <include
                    layout="@layout/layout_ride_summary_new"
                    ></include>

            </RelativeLayout>


            <!-- top bar layout -->
            <include android:id="@+id/topBarMain" layout="@layout/layout_top_bar" />

        </RelativeLayout>

            <include layout="@layout/layout_home_activity_sliding_bottom"/>

        </com.sothree.slidinguppanel.SlidingUpPanelLayout>
    </FrameLayout>

    <!-- menu layout -->
    <LinearLayout
        android:id="@+id/menuLayout"
        android:layout_width="585px"
        android:layout_height="match_parent"
        android:layout_gravity="start"
        android:background="@color/white"
        android:gravity="center_horizontal"
        android:orientation="vertical"
        android:splitMotionEvents="false">

        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center_horizontal"
                android:orientation="vertical"
                android:splitMotionEvents="false">

                <LinearLayout
