<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    android:id="@+id/relative"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="585px"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <RelativeLayout
        android:id="@+id/rlTopContainer"
        android:layout_width="match_parent"
       android:background="#ff834e"
        android:layout_height="wrap_content">





        <RelativeLayout
            android:id="@+id/iv_jugnoo_image"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/px_30"
            android:layout_marginLeft="@dimen/px_50"
            android:layout_marginRight="@dimen/px_25"
            android:layout_marginBottom="@dimen/px_30"
            android:tag="mwar">

<!--            <ImageView
                android:id="@+id/imageViewProfileImageBack"
                android:layout_width="187px"
                android:layout_height="187px"
                android:visibility="gone"
                android:src="@drawable/ic_profile_img_placeholder"
                android:tag="mwar"/>-->

            <ImageView
                android:id="@+id/imageViewProfile"
                android:layout_width="126px"
                android:layout_height="126px"
                android:visibility="visible"
                android:layout_marginLeft="@dimen/px_30"
                android:src="@drawable/ic_profile_img_placeholder"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="@dimen/px_10"
                android:tag="mwar"/>

            <ImageView
                android:id="@+id/viewStarIcon"
                android:layout_width="@dimen/px_60"
                android:layout_height="@dimen/px_60"
                android:layout_marginRight="-40px"
                android:layout_alignBottom="@id/imageViewProfile"
                android:src="@drawable/jugnoo_star_new"
                android:tag="mwar"
                android:visibility="visible" />
        </RelativeLayout>

        <LinearLayout
            android:layout_centerVertical="true"
            android:layout_toRightOf="@id/iv_jugnoo_image"
            android:layout_toLeftOf="@+id/iv_arrow"
            android:layout_width="wrap_content"
            android:layout_marginRight="10px"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <TextView
                android:id="@+id/textViewUserName"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Garry Gurcharan"
                android:maxLines="1"
                android:ellipsize="end"
                android:visibility="visible"
                android:textColor="@color/white"
                android:textSize="@dimen/text_size_34"/>

            <TextView
                android:id="@+id/textViewViewPhone"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="5px"
                android:singleLine="true"
                android:maxLines="1"
                android:ellipsize="end"
                android:text="View Account"
                android:textColor="@color/white"
                android:textSize="@dimen/px_27"/>
        </LinearLayout>


        <ImageView
            android:id="@id/iv_arrow"
            android:layout_width="19px"
            android:layout_height="33px"
            android:layout_alignParentRight="true"
            android:layout_marginRight="30px"
            android:layout_centerVertical="true"
            android:src="@drawable/back_arrow_white"
            android:tag="mwar"/>

    </RelativeLayout>



    <ImageView
        android:id="@+id/stroke"
        android:layout_width="match_parent"
        android:layout_height="2px"
        android:layout_marginBottom="30px"
        android:layout_below="@+id/rlTopContainer"
        android:src="@color/stroke_color_light"/>


    <LinearLayout
        android:id="@+id/linearLayoutCategories"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/stroke"
        android:gravity="center_vertical"
        android:visibility="gone"
        android:orientation="horizontal">

        <ImageView
            android:layout_width="10px"
            android:layout_height="match_parent"
            android:src="@drawable/background_white_theme_selector"/>

        <ImageView
            android:layout_width="90px"
            android:layout_height="81px"
            android:layout_marginLeft="7px"
            android:src="@drawable/ic_menu_category_selector"
            android:tag="mwar"/>

        <TextView
            android:id="@+id/textViewCategories"
            android:layout_width="400px"
            android:layout_height="81px"
            android:layout_centerVertical="true"
            android:gravity="center_vertical"
            android:paddingLeft="14px"
            android:text="Categories"
            android:textColor="@color/text_color_white_theme_selector"
            android:textSize="@dimen/text_size_38"/>

        <ImageView
            android:id="@+id/imageViewArrow"
            android:layout_width="23px"
            android:layout_height="41px"
            android:rotation="270"
            android:src="@drawable/ic_back"
            android:tag="mwar"/>

    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayoutSubCategories"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/px_30"
        android:layout_below="@+id/linearLayoutCategories"
        android:orientation="vertical"
        android:visibility="visible">

        <LinearLayout
            android:id="@+id/linearLayoutSubAutos"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            android:orientation="horizontal">

            <ImageView
                android:layout_width="56px"
                android:layout_height="56px"
                android:layout_marginLeft="40px"
                android:src="@drawable/ic_menu_category_auto_selector"
                android:tag="mwar"
                />

            <TextView
                android:id="@+id/textViewAutos"
                android:layout_width="match_parent"
                android:layout_height="81px"
                android:layout_centerVertical="true"
                android:gravity="center_vertical"

                android:paddingLeft="@dimen/menu_text_margin_offerings"
                android:text="@string/rides"
                android:textColor="@color/text_color_white_theme_selector"
                android:textSize="@dimen/text_size_38"/>

        </LinearLayout>

        <LinearLayout
            android:id="@+id/linearLayoutSubMeals"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            android:orientation="horizontal">

            <ImageView
                android:layout_width="56px"
                android:layout_height="56px"
                android:layout_marginLeft="40px"
                android:src="@drawable/ic_menu_category_meals_selector"
                android:tag="mwar"/>

            <TextView
                android:id="@+id/textViewMeals"
                android:layout_width="match_parent"
                android:layout_height="81px"
                android:layout_centerVertical="true"
                android:gravity="center_vertical"
                android:paddingLeft="@dimen/menu_text_margin_offerings"
                android:text="Meals"
                android:textColor="@color/text_color_white_theme_selector"
                android:textSize="@dimen/text_size_38"/>

        </LinearLayout>

        <LinearLayout
            android:id="@+id/linearLayoutSubFresh"
            android:gravity="center_vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"

            android:orientation="horizontal">

            <ImageView
                android:layout_width="56px"
                android:layout_height="56px"
                android:layout_marginLeft="40px"
                android:src="@drawable/ic_menu_category_fresh_selector"
                android:tag="mwar"/>

            <TextView
                android:id="@+id/textViewFresh"
                android:layout_width="match_parent"
                android:layout_height="81px"
                android:layout_centerVertical="true"
                android:gravity="center_vertical"
                android:paddingLeft="@dimen/menu_text_margin_offerings"
                android:text="@string/fatafat"
                android:textColor="@color/text_color_white_theme_selector"
                android:textSize="@dimen/text_size_38"/>

        </LinearLayout>

        <LinearLayout
            android:id="@+id/linearLayoutSubGrocery"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            android:orientation="horizontal">

            <ImageView
                android:layout_width="56px"
                android:layout_height="56px"
                android:layout_marginLeft="40px"
                android:src="@drawable/ic_menu_category_grocery_selector"
                android:tag="mwar"/>

            <TextView
                android:id="@+id/textViewGrocery"
                android:layout_width="match_parent"
                android:layout_height="81px"
                android:layout_centerVertical="true"
                android:gravity="center_vertical"
                android:paddingLeft="@dimen/menu_text_margin_offerings"
                android:text="@string/grocery"
                android:textColor="@color/text_color_white_theme_selector"
                android:textSize="@dimen/text_size_38"/>

        </LinearLayout>


        <LinearLayout
            android:id="@+id/linearLayoutSubMenus"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            android:orientation="horizontal">

            <ImageView
                android:layout_width="56px"
                android:layout_height="56px"
                android:layout_marginLeft="40px"
                android:src="@drawable/ic_menu_category_menus_selector"
                android:tag="mwar" />

            <TextView
                android:id="@+id/textViewMenus"
                android:layout_width="match_parent"
                android:layout_height="81px"
                android:layout_centerVertical="true"
                android:gravity="center_vertical"
                android:paddingLeft="@dimen/menu_text_margin_offerings"
                android:text="@string/menus"
                android:textColor="@color/text_color_white_theme_selector"
                android:textSize="@dimen/text_size_38"/>

        </LinearLayout>

        <LinearLayout
            android:id="@+id/linearLayoutSubPay"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            android:orientation="horizontal">

            <ImageView
                android:layout_width="56px"
                android:layout_height="56px"
                android:layout_marginLeft="40px"
                android:src="@drawable/ic_menu_category_pay_selector"
                android:tag="mwar"
                android:layout_gravity="center_vertical"
                />

            <TextView
                android:id="@+id/textViewPay"
                android:layout_width="match_parent"
                android:layout_height="81px"
                android:layout_centerVertical="true"
                android:gravity="center_vertical"
                android:paddingLeft="@dimen/menu_text_margin_offerings"
                android:text="@string/pay"
                android:textColor="@color/text_color_white_theme_selector"
                android:textSize="@dimen/text_size_38"/>

        </LinearLayout>

        <LinearLayout
            android:id="@+id/linearLayoutSubFeed"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            android:orientation="horizontal">

            <ImageView
                android:layout_width="56px"
                android:layout_height="56px"
                android:layout_marginLeft="40px"
                android:layout_gravity="center_vertical"
                android:src="@drawable/ic_menu_category_feed_selector"
                android:tag="mwar"/>

            <TextView
                android:id="@+id/textViewFeed"
                android:layout_width="match_parent"
                android:layout_height="81px"
                android:layout_centerVertical="true"
                android:gravity="center_vertical"
                android:paddingLeft="@dimen/menu_text_margin_offerings"
                android:text="@string/ask_local"
                android:textColor="@color/text_color_white_theme_selector"
                android:textSize="@dimen/text_size_38"/>

        </LinearLayout>

    </LinearLayout>

    <View
        android:layout_below="@id/linearLayoutSubCategories"
        android:layout_width="match_parent"
        android:layout_marginLeft="@dimen/px_30"
        android:layout_marginRight="@dimen/px_30"
        android:layout_marginBottom="@dimen/px_30"
        android:background="@color/grey_light_alpha"
        android:layout_height="2px"/>



</RelativeLayout>